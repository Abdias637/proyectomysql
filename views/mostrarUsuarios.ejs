<%- include("templates/encabezado") %>
<%- include("templates/menu") %>
<h1>Mostrar Usuarios</h1>
<table>
    <thead>
        <th>ID</th>
        <th>Nombre</th>
        <th>Usuario</th>
        <th>Password</th>
        <th>Editar / Borrar</th>
    </thead>
    <tbody>
        <%
         usuarios.forEach((usuario)=>{   
        %>
        <tr>
            <td><%= usuario.id %></td>
            <td><%= usuario.nombre %></td>
            <td><%= usuario.usuario %><td>
            <td><%= usuario.password %></td>
            <td><a href="/editarUsuario/<%= usuario.id %>">Editar</a> / <a href="/borrarUsuarios/<%= usuario.id %>">Borrar fisico</a><a href="/borrarUsuarios2/<%= usuario.id %>">Borrar logico</a></td>
        </tr>
        <%
        });
        %>
    </tbody>
</table>
<%- include("templates/pie") %>
